 state Yellow : { background-color: lightyellow; corners: rounded; };
 state Green  : { background-color: lightgreen;  corners: rounded; };
 
 state Off : {
   background-color : steelblue;
   text-color       : white;
   shape            : octagon;
   linestyle        : dashed;
