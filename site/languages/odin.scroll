    switch token {
    case &#39;+&#39;: accumulator += 1;
    case &#39;-&#39;: accumulator -= 1;
    case &#39;*&#39;: accumulator *= 2;
    case &#39;/&#39;: accumulator /= 2;
    case &#39;ðŸ˜ƒ&#39;: accumulator *= accumulator;
